	"editor.suggestSelection": "first",
	"git.autofetch": true,
	"files.trimTrailingWhitespace": true,
	"editor.renderWhitespace": "all",
	"editor.renderControlCharacters": true,
	"extensions.ignoreRecommendations": false,
	"diffEditor.ignoreTrimWhitespace": false,
	"git.confirmSync": false,
	"workbench.statusBar.visible": true,
	"files.exclude": {
		"**/__pycache__": true,
		"**/.classpath": true,
		"**/.dvc": true,
		"**/.factorypath": true,
		"**/.project": true,
		"**/.settings": true,
		"**/*.pyc": true
	},
	"diffEditor.renderSideBySide": true,
	"gitlens.views.repositories.branches.layout": "list",
	"python.languageServer": "Default",
	"[python]": {
		"editor.defaultFormatter": "charliermarsh.ruff",
		"editor.formatOnType": true,
		"diffEditor.ignoreTrimWhitespace": true
	},
	"cSpell.enabled": true,
	"cSpell.userWords": [
		"embedders",
		"hashable",
		"kmeans",
		"openai",
		"ownee",
		"pydantic",
		"vectorizer",
		"vertexai"
	],
	"cSpell.diagnosticLevel": "Hint",
	"jupyter.askForKernelRestart": false,
	"files.watcherExclude": {
		"**.bin": true,
		"**.pyc": true,
		"**.pyd": true,
		"**.pyo": true,
		"**/__pycache__/**": true,
		"**/.dvc/**": true,
		"**/.ipynb_checkpoint/**": true,
		"**/.mypy_cache/**": true
	},
	"git.scanRepositories": [],
	"files.associations": {
		"*.dvc": "yaml",
		"dvc.lock": "yaml",
		"*.psql": "sql",
		"*.pgsql": "sql"
	},
	"workbench.editorAssociations": {
		"*.ipynb": "jupyter-notebook",
		"file:/**/*.parquet": "jupyter-data-wrangler"
	},
	"todo-tree.general.tags": [
		"BUG",
		"HACK",
		"FIXME",
		"TODO",
		"XXX",
		"[ ]",
		"[x]",
		"NOTE",
		"NB"
	],
	"todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
	"editor.cursorStyle": "line",
	"editor.lineNumbers": "on",
	"editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
	"editor.wordWrap": "on",
	"notebook.cellToolbarLocation": {
		"default": "right",
		"jupyter-notebook": "left"
	},
	"gitlens.views.contributors.files.threshold": 7,
	"gitlens.views.contributors.showStatistics": true,
	"notebook.output.textLineLimit": 100,
	"jupyter.widgetScriptSources": ["jsdelivr.com", "unpkg.com"],
	"html.autoClosingTags": false,
	"python.testing.unittestArgs": ["-v", "-s", ".", "-p", "test_*.py"],
	"editor.unicodeHighlight.ambiguousCharacters": false,
	"python.testing.pytestEnabled": true,
	"gitlens.hovers.currentLine.over": "line",
	"explorer.confirmDelete": false,
	"[html]": {
		"editor.tabSize": 2,
		"editor.defaultFormatter": "vscode.html-language-features"
	},
	"files.trimFinalNewlines": true,
	"git.suggestSmartCommit": false,
	"[css]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"editor.minimap.maxColumn": 100,
	"[json]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"eslint.format.enable": true,
	"[javascriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"javascript.updateImportsOnFileMove.enabled": "always",
	"editor.inlineSuggest.enabled": true,
	"[scss]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"gitlens.views.branches.files.layout": "tree",
	"terminal.integrated.enableMultiLinePasteWarning": "auto",
	"[yaml]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"githubPullRequests.pullBranch": "never",
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"workbench.colorTheme": "Default Dark Modern",
	"cloudcode.project": "eilla-dev",
	"[javascript]": {
		"editor.defaultFormatter": "dbaeumer.vscode-eslint"
	},
	"terminal.integrated.env.osx": {
		"FIG_NEW_SESSION": "1",
		"Q_NEW_SESSION": "1"
	},
	"editor.accessibilitySupport": "off",
	"cloudcode.autoDependencies": "off",
	"cloudcode.enableMinikubeGcpAuthPlugin": false,
	"[typescriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"workbench.settings.applyToAllProfiles": [
		"window.zoomLevel",
		"python.analysis.typeCheckingMode"
	],
	"cSpell.language": "en,en-GB",
	"typescript.preferences.importModuleSpecifierEnding": "minimal",
	"terminal.integrated.persistentSessionScrollback": 1000,
	"[shellscript]": {
		"editor.defaultFormatter": "foxundermoon.shell-format"
	},
	"[dockerfile]": {
		"editor.defaultFormatter": "foxundermoon.shell-format"
	},
	"python.interpreter.infoVisibility": "always",
	"githubPullRequests.fileListLayout": "flat",
	"terminal.external.osxExec": "Warp.app",
	"python.terminal.activateEnvironment": false,
	"gitlens.graph.minimap.enabled": false,
	"eslint.lintTask.enable": true,
	"eslint.workingDirectories": ["./frontend"],
	"files.autoSave": "afterDelay",
	"git.mergeEditor": true,
	"[dockercompose]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"notebook.defaultFormatter": "charliermarsh.ruff",
	"diffEditor.maxComputationTime": 0,
	"[terraform]": {
		"editor.defaultFormatter": "hashicorp.terraform"
	},
	"[toml]": {
		"editor.defaultFormatter": "tamasfe.even-better-toml"
	},
	"gitlens.views.branches.branches.layout": "list",
	"editor.acceptSuggestionOnCommitCharacter": false,
	"[markdown]": {
		"editor.defaultFormatter": "yzhang.markdown-all-in-one"
	},
	"[sql]": {
		"editor.defaultFormatter": "cweijan.vscode-postgresql-client2"
	},
	"cursor.cpp.disabledLanguages": ["scminput"],
	"security.promptForLocalFileProtocolHandling": false,
	"cursor.composer.collapsePaneInputBoxPills": true,
	"cursor.cpp.enablePartialAccepts": true,
	"workbench.iconTheme": "material-icon-theme",
	"[go]": {
		"editor.defaultFormatter": "golang.go"
	},
	"editor.unicodeHighlight.invisibleCharacters": false,
	"window.zoomPerWindow": false,
	"database-client.autoSync": true,
	"cursor.composer.shouldChimeAfterChatFinishes": true,
	"ruff.configuration": "",
	"javascript.preferences.importModuleSpecifierEnding": "minimal",
	"cursor.terminal.usePreviewBox": true,
	"debug.onTaskErrors": "debugAnyway",
	"vim.enableNeovim": true,
	"vim.vimrc.enable": true,
	"cursorpyright.analysis.autoImportCompletions": true,
	"cursorpyright.analysis.diagnosticMode": "workspace",
	"cursorpyright.analysis.typeCheckingMode": "strict",
	"gitlens.views.scm.grouped.views": {
		"commits": true,
		"branches": true,
		"remotes": true,
		"stashes": true,
		"tags": true,
		"worktrees": true,
		"contributors": true,
		"fileHistory": true,
		"repositories": true,
		"searchAndCompare": false,
		"launchpad": false
	},
	"application.shellEnvironmentResolutionTimeout": 20,
	"gitlens.ai.model": "gitkraken",
	"gitlens.ai.gitkraken.model": "gemini:gemini-2.0-flash",
	"window.zoomLevel": -1,
	"biome.lsp.trace.server": "messages",
	"biome.requireConfiguration": true,
	"update.releaseTrack": "prerelease",
	"geminicodeassist.inlineSuggestions.enableAuto": false,
	"geminicodeassist.project": "eilla-dev",
	"diffEditor.codeLens": true,
	"geminicodeassist.displayInlineContextHint": false,
	"claudeCode.selectedModel": "haiku"
}
